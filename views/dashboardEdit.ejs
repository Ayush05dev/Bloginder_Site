<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head') %>
    <title>Dashboard</title>
</head>
<body>
    <%- include('./partials/nav') %>
    <div class="container mt-3" >
        <form action="/user/dashboard/<%= user._id %>" method="post" enctype="multipart/form-data">
            <div class="mb-3">
                <label for="profileImageURL" class="form-label">Profile Image</label>
                <input class="form-control" type="file" id="profileImageURL" name="profileImageURL">
              </div>
            <div class="mb-3">
                <label for="fullName" class="form-label">FullName</label>
                <input 
                type="text"
                class="form-control"
                id="fullName"
                name="fullName"
                aria-describedby="fullName" 
                />
            </div>
              <div class="mb-3">
                <button type="submit" class="btn btn-primary">Submit</button>
              </div>
        </form>
    </div>
    <%- include('./partials/script') %>
</body>
</html>
